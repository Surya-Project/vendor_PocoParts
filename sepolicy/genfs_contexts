# Touch profiles
genfscon proc /nvt_sensitivity_switch                                    u:object_r:proc_nvt_feature:s0
genfscon proc /nvt_pf_switch                                             u:object_r:proc_nvt_feature:s0
genfscon proc /nvt_er_range_switch                                       u:object_r:proc_nvt_feature:s0

# lcd
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display/hbm            u:object_r:sysfs_lcd_feature:s0
genfscon sysfs /devices/platform/soc/soc:qcom,dsi-display/cabc           u:object_r:sysfs_lcd_feature:s0

# FPS Info
genfscon sysfs /sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/sde-crtc-0/measured_fps u:object_r:vendor_sysfs_graphics:s0

# Haptic
genfscon sysfs /devices/platform/soc/884000.i2c/i2c-3/3-005a/ulevel                              u:object_r:sysfs_vibrator:s0
